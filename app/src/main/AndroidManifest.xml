<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.manuela.timemanager">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InformationUs"></activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/login_title" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/register_title" />
        <activity
            android:name=".ProfileActivity"
            android:label="@string/profile_title" />
        <activity android:name=".AddTaskActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".ThingsToDoActivity" />
        <activity android:name=".Configurations" />
        <activity android:name=".General" />
        <activity android:name=".Notificaciones" />
        <activity android:name=".EditarInformacion" />
    </application>

</manifest>